<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="345dp"
    android:layout_height="185dp"
    android:layout_gravity="center"
    android:gravity="center">


    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="center"
        android:gravity="center">
        <Button
            android:id="@+id/btn_weight"
            android:layout_width="115dp"
            android:layout_height="115dp"
            android:layout_marginTop="35dp"
            android:layout_marginLeft="80dp"
            android:fontFamily="@font/orbitronbold"
            android:textColor="@android:color/background_light"
            android:textSize="30sp"
            android:text="100"
            android:background="@drawable/btn_setbig_nopress"/>
        <Button
            android:id="@+id/btn_startPosition"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_marginTop="5dp"
            android:layout_marginLeft="5dp"
            android:fontFamily="@font/orbitronbold"
            android:textColor="@android:color/background_light"
            android:textSize="20sp"
            android:text="888"
            android:background="@drawable/btn_setsmall_nopress"/>
        <Button
            android:id="@+id/btn_setConcentricSpeed"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_marginTop="5dp"
            android:layout_marginLeft="190dp"
            android:fontFamily="@font/orbitronbold"
            android:textColor="@android:color/background_light"
            android:textSize="20sp"
            android:text="888"
            android:background="@drawable/btn_setsmall_nopress"/>
        <Button
            android:id="@+id/btn_endPosition"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_marginTop="95dp"
            android:layout_marginLeft="5dp"
            android:fontFamily="@font/orbitronbold"
            android:textColor="@android:color/background_light"
            android:textSize="20sp"
            android:text="888"
            android:background="@drawable/btn_setsmall_nopress"/>

        <Button
            android:id="@+id/btn_setEccentricSpeed"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_marginTop="95dp"
            android:layout_marginLeft="190dp"
            android:background="@drawable/btn_setsmall_nopress"
            android:fontFamily="@font/orbitronbold"
            android:text="888"
            android:textColor="@android:color/background_light"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/txt_nowPosition"
            android:layout_width="115dp"
            android:layout_height="25dp"
            android:layout_marginTop="150dp"
            android:layout_marginLeft="80dp"
            android:gravity="center"
            android:fontFamily="@font/orbitronbold"
            android:textColor="@android:color/background_light"
            android:textSize="15sp"
            android:text="888"/>

    </FrameLayout>


    <TextView
        android:layout_width="115dp"
        android:layout_height="25dp"
        android:layout_marginTop="110dp"
        android:layout_marginLeft="80dp"
        android:gravity="center"
        android:fontFamily="@font/orbitronbold"
        android:textColor="@android:color/background_light"
        android:textSize="20sp"
        android:text="kg"/>
    <TextView
        android:layout_width="80dp"
        android:layout_height="30dp"
        android:layout_marginTop="55dp"
        android:layout_marginLeft="5dp"
        android:gravity="center"
        android:fontFamily="@font/orbitronregular"
        android:textColor="@android:color/background_light"
        android:letterSpacing="0.1"
        android:textSize="9sp"
        android:text="START\nPOSITION"/>
    <TextView
        android:layout_width="90dp"
        android:layout_height="30dp"
        android:layout_marginTop="55dp"
        android:layout_marginLeft="185dp"
        android:gravity="center"
        android:fontFamily="@font/orbitronregular"
        android:textColor="@android:color/background_light"
        android:letterSpacing="0.1"
        android:textSize="9sp"
        android:text="CONCCETRIC\nSPEED"/>
    <TextView
        android:layout_width="80dp"
        android:layout_height="30dp"
        android:layout_marginTop="145dp"
        android:layout_marginLeft="5dp"
        android:gravity="center"
        android:fontFamily="@font/orbitronregular"
        android:textColor="@android:color/background_light"
        android:letterSpacing="0.1"
        android:textSize="9sp"
        android:text="END\nPOSITION"/>
    <TextView
        android:layout_width="80dp"
        android:layout_height="30dp"
        android:layout_marginTop="145dp"
        android:layout_marginLeft="190dp"
        android:gravity="center"
        android:fontFamily="@font/orbitronregular"
        android:textColor="@android:color/background_light"
        android:letterSpacing="0.1"
        android:textSize="9sp"
        android:text="ECCENTRIC\nSPEED"/>


    <LinearLayout
        android:layout_width="60dp"
        android:layout_height="230px"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="370px"
        android:orientation="vertical">

        <Button
            android:id="@+id/btn_upHigh"
            android:layout_width="80px"
            android:layout_height="50px"
            android:gravity="center"
            android:fontFamily="@font/orbitronbold"
            android:textColor="@android:color/background_light"
            android:textSize="25px"
            android:text="+5"
            android:background="@drawable/btn_set"/>

        <Button
            android:id="@+id/btn_upLow"
            android:layout_width="80px"
            android:layout_height="50px"
            android:layout_marginTop="10px"
            android:gravity="center"
            android:fontFamily="@font/orbitronbold"
            android:textColor="@android:color/background_light"
            android:textSize="25px"
            android:text="+1"
            android:background="@drawable/btn_set"/>

        <Button
            android:id="@+id/btn_dnLow"
            android:layout_width="80px"
            android:layout_height="50px"
            android:layout_marginTop="10px"
            android:gravity="center"
            android:fontFamily="@font/orbitronbold"
            android:textColor="@android:color/background_light"
            android:textSize="25px"
            android:text="-1"
            android:background="@drawable/btn_set"/>

        <Button
            android:id="@+id/btn_dnHigh"
            android:layout_width="80px"
            android:layout_height="50px"
            android:layout_marginTop="10px"
            android:gravity="center"
            android:fontFamily="@font/orbitronbold"
            android:textColor="@android:color/background_light"
            android:textSize="25px"
            android:text="-5"
            android:background="@drawable/btn_set"/>
    </LinearLayout>

</FrameLayout>